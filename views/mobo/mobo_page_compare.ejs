<html> 
	<head> 
			<!-- Bootstrap CSS -->
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
	<!-- BOOTSTRAP ICON FONT -->
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.2/font/bootstrap-icons.css">

	<!-- Bootstraps script -->
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
	<!-- TABLE SORT SCRIPT -->
	<script src="https://leewannacott.github.io/table-sort-js/table-sort.js"></script>
	</head>

	<body> 
		<!-- HEADER -->
		<div class="container">
			<header class="d-flex flex-wrap align-items-center justify-content-center justify-content-md-between py-3 mb-4 border-bottom">
				<a href="/" class="d-flex align-items-center col-md-3 mb-2 mb-md-0 text-dark text-decoration-none">
					<!-- <svg class="bi me-2" width="40" height="32" role="img" aria-label="Bootstrap"><use xlink:href="#bootstrap"></use></svg> -->
				</a>
	
				<ul class="nav col-12 col-md-auto mb-2 justify-content-center mb-md-0">
					<li><a href="#" class="nav-link px-2 link-secondary">Conversational Recommender System - PC Specification</a></li>
				</ul>
	
				<div class="col-md-3 text-end">
					<!-- <button type="button" class="btn btn-outline-primary me-2">Login</button> -->
					<!-- <button type="button" class="btn btn-primary">Sign-up</button> -->
				</div>
			</header>
		</div>
		<form class=" bg-white px-4" action="/crs/ram-checkpoint" method="POST">
			<div class="container">
				<section class="text-center">
					<h4 class="mb-4"><strong>Perbandingan Produk</strong></h4>
					<div class="btn-group mb-4" role="group" aria-label="Basic example">
						<button class=" btn btn-dark" type="button" data-bs-target="#carouselTableControls" data-bs-slide-to="0">Spesifikasi Produk</button>
					</div>
				
					<div id="carouselTableControls" class="carousel slide">
						<div class="carousel-inner">
						  <div class="carousel-item active">
							<div class="table-responsive">
								<table class="table">
									<thead>
										<tr>
											<th scope="col">Product Specification</th>
											<% for(i=0;i<queries.length;i++){ %>
											  <th scope="col"><%= queries[i][0].properties['Name']%> %></th>
											<% } %> 
											</tr>
									</thead>
									<tbody>
									  <tr>
										<th scope="row">CPU Socket</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['CPU Socket']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Chipset</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['Chipset']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Harga Rata-Rata</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%= queries[i][1]%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Crossfire Support</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['Crossfire Support']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">SLI Support</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['SLI Support']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Form Factor</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['Form Factor']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Manufacturer</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['Manufacturer']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Maximum Supported Memory</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['Maximum Supported Memory']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Memory Pin Types</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['Memory Pin Types']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Memory Slots</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['Memory Slots']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Memory Speed</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['Memory Speed']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Memory Type</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['Memory Type']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Onboard Ethernet</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['Onboard Ethernet']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Onboard USB 3.0 Header(s)</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['Onboard USB 3.0 Header(s)']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">Onboard Video</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['Onboard Video']%></td>
										  <% } %> 
									  </tr>
									  <tr>
										<th scope="row">RAID Support</th>
										<% for(i=0;i<queries.length;i++){ %>
											<td><%=queries[i][0].properties['RAID Support']%></td>
										  <% } %> 
									  </tr>
									</tbody>
									<tr>
									  <th scope="row"></th>
									  <% for(i=0;i<queries.length;i++){ %>
											<td><button class="btn btn-dark" id="finalMobo"  name="finalMobo" type="submit" value="<%= queries[i][2] %>" data-bs-toggle="modal" data-bs-target="#modalSubmit">Choose Motherboard</button></td>
										<% } %> 
									</tr>
								</table>
							</div>
						  </div>
						</div>
					</div>
				</div>
			</div>
		</form>
		</section>

	</body>

</html>

	